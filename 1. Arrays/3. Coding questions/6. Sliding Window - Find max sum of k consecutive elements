//APPROACH 1 - BRUTE 

public class maxSumKElements{ 
	public static void main(String[] args) { 
		int[] nums = {5,10,1,388,99};
		int k = 2;
		int currSum;
		int maxSum = 0;
		for(int i = 0; i+k-1<nums.length; i++)
		{
			currSum = 0;
			for(int j = i; j<i+k; j++)
			{
				currSum += nums[j];
			}
			maxSum = Math.max(currSum,maxSum);
		}
		System.out.println(maxSum);
	}
}

