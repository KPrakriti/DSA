//Prepbytes

import java.util.*;
import java.lang.*;
import java.io.*;

public class binary { 

	public static void main(String[] args) { 
		
	Scanner sc=new Scanner(System.in);
	int T=sc.nextInt();
		while(T-->0)
		{
			int N=sc.nextInt();
			int[] arr=new int[N];
			arr[0]=0;
			getBinary(arr,1,N);
			arr[0]=1;
			getBinary(arr,1,N);
			System.out.println();
		}	
	}
	private static void getBinary(int[] arr, int i, int N)
	{
	  //base case
		if(i==N)
		{
			print_arr(arr);
			return;
		}
		if(arr[i-1]==0)
		{
			arr[i]=0;
			getBinary(arr,i+1,N);
			arr[i]=1;
			getBinary(arr,i+1,N);
		}
		else if(arr[i-1]!=0)
		{
			arr[i]=0;
			getBinary(arr,i+1,N);
		}
	}
	private static void print_arr(int[] arr)
	{
		int len=arr.length;
		for(int i=0; i<len; i++)
		{
			System.out.print(arr[i]);
		}
		System.out.println();
	}
}
