//Pepcoding

import java.io.*;
import java.util.*;

public class Main {

    public static void main(String[] args) throws Exception {
        // write your code here
        Scanner sc=new Scanner(System.in);
        int n= sc.nextInt();
        int ans=factorial(n);
        System.out.println(ans);
    }

    public static int factorial(int n){
        int fact =n;
        if(n==1) return 1;
        
        fact = fact*factorial(n-1);
        return fact;
    }

}
