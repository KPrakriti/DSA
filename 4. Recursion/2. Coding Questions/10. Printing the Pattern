//Prepbytes

import java.util.*;
import java.lang.*;
import java.io.*;

public class printPattern { 

	public static void main(String[] args) { 
		
	Scanner sc=new Scanner(System.in);
	int T=sc.nextInt();
		while(T-->0)
		{
			int N=sc.nextInt();
			printSeq(N);
			System.out.println();
		}	
	}
	private static void printSeq(int N)
	{
		System.out.print(N+" ");
		if(N<=0) return;
		printSeq(N-5);
		System.out.print(N+" ");
	
	}
	
}
